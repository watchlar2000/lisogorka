{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSameLine": false,
	"htmlWhitespaceSensitivity": "ignore",
	"tabWidth": 2,
	"printWidth": 80,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
